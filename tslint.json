{
  "rules": {
    "interface-name": false,
    "no-console": false,
    "no-empty": [
      true,
      "allow-empty-functions"
    ]
  },
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
